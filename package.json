{
  "name": "flexpay-calendar",
  "version": "0.1.0",
  "description": "Calendar.io test",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/TypeScript-Node-Starter"
  },
  "homepage": "./",
  "proxy": "http://localhost:3001",
  "author": "Manuel Sanchez",
  "license": "MIT",
  "scripts": {
    "watch-node": "nodemon ./server/dist/server.js",
    "start": "concurrently -k -p \"[{name}]\" -n \"React,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run start:server\" \"npm run start:client\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "build:clean": "./node_modules/.bin/rimraf dist",
    "build:ts": "tsc --project ./server/tsconfig.json",
    "build": "yarn build:clean && yarn build:ts",
    "start:server": "yarn build && node ./server/dist/server.js | pino-colada",
    "start:client": "react-scripts start",
    "build:client": "react-scripts build",
    "test:client": "react-scripts test",
    "eject:client": "react-scripts eject",
    "server:client": "node-env-run server --exec nodemon | pino-colada",
    "dev:client": "run-p server start"
  },
  "dependencies": {
    "@types/morgan": "^1.7.35",
    "async": "^2.6.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.2",
    "express-async-router": "^0.1.15",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "mongoose": "^5.4.2",
    "morgan": "^1.9.1",
    "ramda": "^0.26.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "^2.1.8",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/async": "^2.0.45",
    "@types/body-parser": "^1.16.8",
    "@types/compression": "^0.0.35",
    "@types/dotenv": "^4.0.2",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.11.1",
    "@types/lusca": "^1.5.0",
    "@types/node": "^9.4.6",
    "@types/request": "^2.47.0",
    "@types/supertest": "^2.0.4",
    "@types/winston": "^2.3.7",
    "concurrently": "^3.5.1",
    "express-pino-logger": "^4.0.0",
    "node-env-run": "^3.0.2",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "pino-colada": "^1.4.4",
    "rimraf": "^2.6.3",
    "supertest": "^3.0.0",
    "ts-node": "^5.0.0",
    "tslint": "^5.9.1",
    "typescript": "^3.4.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
